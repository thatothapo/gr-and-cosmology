Cell[TextData[{
 "We write the following metric in matrix form (can change to whichever \
metric you are using)\n\t\t ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox[
     SuperscriptBox["ds", "2"],
     FontSize->24,
     FontWeight->"Bold"], 
    StyleBox["=",
     FontSize->24,
     FontWeight->"Bold"], 
    RowBox[{
     RowBox[{
      StyleBox["-",
       FontSize->24,
       FontWeight->"Bold"], 
      SuperscriptBox[
       StyleBox["dt",
        FontSize->24,
        FontWeight->"Bold"], 
       StyleBox["2",
        FontSize->14,
        FontWeight->"Bold"]]}], 
     StyleBox["+",
      FontSize->24,
      FontWeight->"Bold"], 
     RowBox[{
      RowBox[{
       SuperscriptBox[
        StyleBox["a",
         FontSize->24,
         FontWeight->"Bold"], 
        StyleBox["2",
         FontSize->14,
         FontWeight->"Bold"]], 
       StyleBox["(",
        FontSize->24,
        FontWeight->"Bold"], 
       StyleBox["t",
        FontSize->24,
        FontWeight->"Bold"], 
       StyleBox[")",
        FontSize->24,
        FontWeight->"Bold"]}], 
      StyleBox["[",
       FontSize->24,
       FontWeight->"Bold"], 
      RowBox[{
       FractionBox[
        StyleBox[
         SuperscriptBox["dr", "2"],
         FontSize->24,
         FontWeight->"Bold"], 
        StyleBox[
         RowBox[{"1", "-", 
          SuperscriptBox["kr", "2"]}],
         FontSize->18,
         FontWeight->"Bold"]], 
       StyleBox["+",
        FontSize->24,
        FontWeight->"Bold"], 
       StyleBox[
        RowBox[{
         SuperscriptBox["r", "2"], 
         SuperscriptBox["d\[Theta]", "2"]}],
        FontSize->24,
        FontWeight->"Bold"], 
       StyleBox["+",
        FontSize->24,
        FontWeight->"Bold"], 
       StyleBox[
        RowBox[{
         SuperscriptBox["r", "2"], 
         SuperscriptBox["sin", "2"], 
         SuperscriptBox["\[Theta]d\[Phi]", "2"]}],
        FontSize->24,
        FontWeight->"Bold"]}], 
      StyleBox["]",
       FontSize->24,
       FontWeight->"Bold"]}]}]}], TraditionalForm]],ExpressionUUID->
  "ad59f82f-5cc7-4dc9-90fe-c6a3fcef20f6"],
 " \nThis is the FRW metric (can input any other metric in its place)"
}], "Text",
