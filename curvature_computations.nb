67ff78e8e967"],

Cell[BoxData[
 RowBox[{
  RowBox[{"cc", "[", 
   RowBox[{"a_", ",", "b_", ",", "c_"}], "]"}], " ", ":=", " ", 
  RowBox[{
   RowBox[{"cc", "[", 
    RowBox[{"a", ",", "b", ",", "c"}], "]"}], "=", 
   RowBox[{"Simplify", "[", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"d", "=", "1"}], "n"], 
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{"inv", "[", 
       RowBox[{"[", 
        RowBox[{"a", ",", "d"}], "]"}], "]"}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"metric", "[", 
           RowBox[{"[", 
            RowBox[{"d", ",", "c"}], "]"}], "]"}], ",", 
          RowBox[{"coord", "[", 
           RowBox[{"[", "b", "]"}], "]"}]}], "]"}], "+", 
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"metric", "[", 
           RowBox[{"[", 
            RowBox[{"d", ",", "b"}], "]"}], "]"}], ",", 
          RowBox[{"coord", "[", 
           RowBox[{"[", "c", "]"}], "]"}]}], "]"}], "-", 
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"metric", "[", 
           RowBox[{"[", 
            RowBox[{"b", ",", "c"}], "]"}], "]"}], ",", 
          RowBox[{"coord", "[", 
           RowBox[{"[", "d", "]"}], "]"}]}], "]"}]}], ")"}]}]}], 
    "]"}]}]}]], "Input",
